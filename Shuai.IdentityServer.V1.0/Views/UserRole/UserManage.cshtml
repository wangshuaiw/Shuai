
@{
    ViewData["Title"] = "管理用户";
}

<h3>@ViewData["Title"]</h3>

<div class="row">
    <form class="container-fluid">
        <div class="form-row">
            <div class="form-group col-md-4">
                <label>用户名</label>
                <input type="text" class="form-control" id="inputUserName" />
            </div>
            <div class="form-group col-md-4">
                <label>邮箱</label>
                <input type="email" class="form-control" id="inputEmail" />
            </div>
            <div class="form-group col-md-4">
                <label>手机号</label>
                <input type="text" class="form-control" id="inputPhone" />
            </div>
        </div>
        <button type="submit" class="offset-11 btn btn-primary">搜索</button>
    </form>
</div>
<div class="row">
    <table class="table" style="margin-top:20px;">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">用户名</th>
                <th scope="col">邮箱</th>
                <th scope="col">手机号</th>
                <th scope="col">角色</th>
                <th scope="col">操作</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th scope="row">1</th>
                <td>SuperAdmin</td>
                <td>SuperAdmin@email.com</td>
                <td>0000000000</td>
                <td>SuperAdmins</td>
                <td>修改|删除</td>
            </tr>
        </tbody>
    </table>
</div>
